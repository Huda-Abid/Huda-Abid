#include<iostream>
using namespace std;
int main()
{
int T={1,2,3,4,5,6,6,7,4,2};
int P={6,6};
int n=10;
int m=2;
int q=11;
int h=(10^(m-1))%q;
int p=0;
int t=0;
for (int i=0;i<m;i++)
{
  p=(10*p+P[i])%q;
  t=(10*t+T[i])%q;
}
for (s=0;s<n-m;s++)
{
  if (p==t)
  {
    for (int i=0;i<m;i++)
    {
      if(T[s+i]!=P[i])
        break;
    }
    if(i==m)
    {
      cout<"Pattern occurs"<<endl;
    }
  }
  if(s<n-m)
  {
    t=(10*(t-T[s+1]*h)+T[s+m+1])%q;
  }
}
return 0;
}
